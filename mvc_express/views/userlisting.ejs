<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile Update</title>
    <style>
        body{
            background: grey;
        }
    </style>
</head>
<body>
    <center>

    <h1>User  List
           <a href="/user/home">Home</a>

    </h1>
         <table border="1">
            <tr>
             <th>Sno</th>
             <th>Name</th>
             <th>Email</th>
             <th>Phone</th>
             <th>Password</th>
             <th>Action</th>
             </tr>
              
         <% users.forEach(function(user) { %>

             <tr>
                 <td>#</td>
                 <td><%=user.name%></td>
                 <td><%=user.email%></td>
                 <td><%=user.phone%></td>
                 <td><%=user.password%></td>
                 <td>
                  <a href="deluser/<%=user.id%>">Delete</a>
                  <br>
                  <a href="edituser/<%=user.id%>">Edit</a>
                 </td>
             </tr>

            <% }) %>

         </table>

        </center>
       


</body>
</html>
