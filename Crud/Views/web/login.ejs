<body class="login-body">

<div class="container">
<div class="login-wrapper">
<div class="row justify-content-center">
<div class="col-lg-6 col-md-8">


<div class="login-box">

<h2>Login</h2>


<form action="home/user_login" method="post">
<div class="form-group">
	<input type="email" name="email" value="" placeholder="Email Address" maxlength="30" class="form-control" placeholder="EMAIL">


</div>
<div class="form-group">
	<input type="password" name="password" value="" class="form-control" placeholder="PASSWORD">

</div>

<div class="mb-2 text-center"><input type="submit" class="btn btn-dark" value="LOGIN NOW"></div>

</form>

</div>


</div>
</div>
</div>
</div>

